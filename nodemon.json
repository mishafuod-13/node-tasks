{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
        "js": "node --harmony",
        "ts": "ts-node"
    },
    "delay": 5000,
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ts,js,json"
}